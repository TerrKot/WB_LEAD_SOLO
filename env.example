# Redis Configuration
# Для локальной разработки: redis://localhost:6379/0
# Для Docker (используется встроенный Redis контейнер):
#   - Контейнер wb_lead_redis автоматически создаётся в docker-compose.yml
#   - Используй имя сервиса: redis://redis:6379/0
REDIS_URL=redis://redis:6379/0

# PostgreSQL Configuration
# Для локальной разработки: postgresql+asyncpg://app:app@localhost:5433/app
# Для Docker (используется встроенный PostgreSQL контейнер):
#   - Контейнер wb_lead_postgres автоматически создаётся в docker-compose.yml
#   - Используй имя сервиса: postgresql+asyncpg://app:app@postgres:5432/app
DATABASE_URL=postgresql+asyncpg://app:app@postgres:5432/app

# Exchange Rates
# Курсы валют (можно обновлять через API или вручную)
EXCHANGE_RATE_USD_RUB=100.0
EXCHANGE_RATE_USD_CNY=7.2
EXCHANGE_RATE_EUR_RUB=110.0

# White Logistics Configuration
# Базовая цена белой логистики (1000 кг / 4.6 м³)
WHITE_LOGISTICS_BASE_PRICE_USD=1850
# Стоимость документов в рублях
WHITE_LOGISTICS_DOCS_RUB=15000
# Стоимость брокера в рублях
WHITE_LOGISTICS_BROKER_RUB=25000

# Service Configuration
# Порт для health endpoint и webhook (внутри контейнера, внешний порт 8443)
SERVICE_PORT=8444
# Уровень логирования: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
